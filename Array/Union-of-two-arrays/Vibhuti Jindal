//The function doUnion takes two arrays A of size n and B of size m and return the size of the union array

int doUnion(int a[], int n, int b[], int m) 
{
        set<int> s;
       for(int i=0;i<n;i++)
       {
           s.insert(a[i]);
       }
       for(int i=0;i<m;i++)
       {
           s.insert(b[i]);
       }
       return s.size();
}
